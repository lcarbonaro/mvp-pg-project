{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/menu",
      "destination": "/backend/src/server.js"
    },
    {
      "source": "/api/menu/:id",
      "destination": "/backend/src/server.js"
    }
  ]
}